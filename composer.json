{
    "name": "silverd/laravel-sortable",
    "description": "Sortable behaviour for eloquent models",
    "homepage": "https://github.com/silverd/laravel-sortable",
    "authors": [
        {
            "name": "silverd",
            "email": "silverd29@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "illuminate/database": "~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Silverd\\LaravelSortable\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Silverd\\LaravelSortable\\ServiceProvider"
            ]
        }
    }
}
